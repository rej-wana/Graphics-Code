//////////////////////////////////////////////////////////////////



void MidPointCircle()
{

  int gd = DETECT, gm;
    initgraph(&gd, &gm, "");
    initwindow(1024, 1024, "Midpoint Circle Algorithm");

    int xc, yc, r;
    printf("Enter the Center of The Circle(xc,yc):");
    scanf("%d%d",&xc,&yc);

    printf("Enter The Radius of The Circle(r):");
    scanf("%d",&r);
    //drawcircle(xc,yc,r);

    int x = 0, y = r;
    int d = 1 - r;

    auto plotPoints = [&](int xc, int yc, int x, int y) {
        putpixel(xc + x, yc + y, WHITE);
        putpixel(xc - x, yc + y, WHITE);
        putpixel(xc + x, yc - y, WHITE);
        putpixel(xc - x, yc - y, WHITE);
        putpixel(xc + y, yc + x, WHITE);
        putpixel(xc - y, yc + x, WHITE);
        putpixel(xc + y, yc - x, WHITE);
        putpixel(xc - y, yc - x, WHITE);
        delay(100);
    };

    plotPoints(xc, yc, x, y);

    while (x < y) {
        x++;
        if (d < 0) {
            d += 2 * x + 1;
        } else {
            y--;
            d += 2 * (x - y) + 1;
        }
        plotPoints(xc, yc, x, y);
    }


}





//////////////////////////////////////////////////////////////////


void DDA()
{
    float x1, y1, x2, y2;
    printf("Enter the value of x1 and y1 :");
    scanf("%f%f",&x1,&y1);

    printf("Enter the value of x2 and y2 :");
    scanf("%f%f",&x2,&y2);

    float dx = abs(x2 - x1);
    float dy = abs(y2 - y1);
    float step = (dx > dy) ? dx : dy;
    dx /= step;
    dy /= step;

    float x = x1;
    float y = y1;
    int i = 1;

    while (i < step) {
        putpixel(x, y, WHITE);
        x += dx;
        y += dy;
        i++;
        delay(200);
    }



}




